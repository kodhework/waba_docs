            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": [
                      "document",
                      "image",
                      "audio",
                      "text",
                      "video"
                    ]
                  },
                  "base64": {
                    "type": "string",
                    "description": "Data url representation of attached  file if required"
                  },
                  "body": {
                    "type": "string",
                    "description": "Text of message"
                  },
                  "caption": {
                    "type": "string",
                    "description": "Caption of an image, document, or video"
                  },
                  "number": {
                    "type": "string",
                    "description": "Number with country code, but without +"
                  },
                  "chatId": {
                    "type": "string",
                    "description": "ChatId. Ex: 111111111@c.us If not passed generated from number"
                  }
                },
                "required": [
                  "type"
                ]
              }
            }
          }
        }
      }
