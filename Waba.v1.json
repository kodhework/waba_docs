        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "query": {
                    "title": "MessageQuery",
                    "type": "object",
                    "properties": {
                      "\"from.uid\"": {
                        "type": "string",
                        "description": "Filter by received from"
                      },
                      "\"to.uid\"": {
                        "type": "string",
                        "description": "Filter by sent to"
                      },
                      "\"id.remote.uid\"": {
                        "type": "string",
                        "description": "Filter by id from remote user. See MessageKey model"
                      },
                      "messageNumber": {
                        "anyOf": [
                          {
                            "type": "number"
                          },
                          {
                            "title": "GtQuery",
                            "type": "object",
                            "properties": {
                              "$gt": {
                                "type": [
                                  "number",
                                  "string",
                                  "integer"
                                ]
                              }
                            }
                          },
                          {
                            "title": "GteQuery",
                            "type": "object",
                            "properties": {
                              "$gte": {
                                "type": [
                                  "string",
                                  "number",
                                  "integer"
                                ]
                              }
                            }
                          },
                          {
                            "title": "LtQuery",
                            "type": "object",
                            "properties": {
                              "$lt": {
                                "type": [
                                  "string",
                                  "number",
                                  "integer"
                                ]
                              }
                            }
                          },
                          {
                            "title": "LteQuery",
                            "type": "object",
                            "properties": {
                              "$lte": {
                                "type": [
                                  "string",
                                  "number",
                                  "integer"
                                ]
                              }
                            }
                          }
                        ]
                      }
                    }
                  },
                  "sort": {
                    "title": "Sort",
                    "type": "object",
                    "properties": {
                      "messageNumber": {
                        "type": "number",
                        "enum": [
                          -1,
                          1
                        ]
                      },
                      "t": {
                        "type": "number",
                        "enum": [
                          1,
                          -1
                        ]
                      }
                    }
                  },
                  "limit": {
                    "type": "number"
                  }
                }
              }
            }
          }
        }
      }
